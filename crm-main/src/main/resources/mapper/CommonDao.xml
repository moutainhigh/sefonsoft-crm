<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sefonsoft.oa.dao.common.CommonDao">

    <!--通过实体作为筛选条件查询-->
    <select id="queryAllEnterpriseType" resultType="com.sefonsoft.oa.entity.customer.EnterpriseType" parameterType="com.sefonsoft.oa.entity.customer.EnterpriseType">
        select
          enter_id, enter_name
        from sefonoa.enterprise_type
    </select>
</mapper>